<h1>Log In:</h1>

<%= render 'shared/errors' %>

<%= form_tag({controller: "sessions", action: "create"}, class: "login_form") do %>
  <%= label_tag(:username)%>
  <%= text_field_tag(:username)%>
  <%= label_tag(:password)%>
  <%= password_field_tag(:password)%>
  <%= submit_tag("Log In") %>
<% end %>
